<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- 默认时的背景图片 -->

    <item android:color="#666666" android:state_checked="false"/>
    <!-- 选中时的图片背景 -->
    <item android:color="@color/themecolor" android:state_selected="true"/>
    <item android:color="@color/themecolor" android:state_pressed="true"/>
    <item android:color="@color/themecolor" android:state_checked="true"/>
</selector>